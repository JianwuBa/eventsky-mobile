K,MAAD,CAAd;AACH;AArCI,CAAb,C,CAuCI;;AACJ,eAAeJ,IAAf","sourcesContent":["/****   http.js   ****/\n// 导入封装好的axios实例\nimport request from './request'\n\nconst http = {\n        /**\n         * methods: 请求\n         * @param url 请求地址 \n         * @param params 请求参数\n         */\n        get(url, params) {\n            const config = {\n                method: 'get',\n                url: url\n            }\n            if (params) config.params = params\n            return request(config)\n        },\n        post(url, params) {\n            const config = {\n                method: 'post',\n                url: url\n            }\n            if (params) config.data = params\n            return request(config)\n        },\n        put(url, params) {\n            const config = {\n                method: 'put',\n                url: url\n            }\n            if (params) config.params = params\n            return request(config)\n        },\n        delete(url, params) {\n            const config = {\n                method: 'delete',\n                url: url\n            }\n            if (params) config.params = params\n            return request(config)\n        }\n    }\n    //导出\nexport default http"]}]}